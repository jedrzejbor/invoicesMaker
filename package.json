{
  "name": "fakturke-se-wystaw",
  "version": "1.0.0",
  "private": true,
  "description": "Aplikacja do cyklicznego wystawiania faktur",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "pnpm --filter @fakturke/api db:generate",
    "db:migrate": "pnpm --filter @fakturke/api db:migrate",
    "db:seed": "pnpm --filter @fakturke/api db:seed",
    "db:studio": "pnpm --filter @fakturke/api db:studio"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
